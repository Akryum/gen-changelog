{
  "name": "@akryum/sheep",
  "version": "0.5.2",
  "description": "Opinionated CLI command to release and update CHANGELOG.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/Akryum/sheep.git"
  },
  "license": "MIT",
  "author": {
    "name": "Guillaume Chau"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "sheep": "./bin.js"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c && chmod +x ./bin.js",
    "watch": "rollup -c --watch",
    "release": "pnpm run build && node ./dist/bin.js release -b main"
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@types/cross-spawn": "^6.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^14.14.21",
    "@types/prompts": "^2.0.14",
    "@types/semver": "^7.3.9",
    "changelogen": "^0.5.5",
    "esbuild": "^0.19.11",
    "execa": "^6.1.0",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.0.0",
    "node-fetch": "^3.3.1",
    "pathe": "^0.2.0",
    "picocolors": "^1.0.0",
    "prompts": "^2.4.2",
    "rimraf": "^3.0.2",
    "rollup": "^4.9.5",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "sade": "^1.8.1",
    "semver": "^7.3.5",
    "typescript": "^4.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "bin.js"
  ]
}
